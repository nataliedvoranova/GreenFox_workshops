@using tododatabase.Models
@model List<Todo>

<!DOCTYPE html>
<html>

<head>

</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Title</th>
                <th>Descrption</th>
                <th>Urgent?</th>
                <th>Done?</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Title</td>
                    <td>@item.Description</td>
                    <td>@item.IsUrgent</td>
                    <td>@item.IsDone</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>


